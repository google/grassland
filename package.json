{
  "name": "grassland",
  "version": "0.1.0",
  "description": "Git Static Distribution",
  "main": "dist/index.js",
  "dependencies": {
    "@types/express": "^4.16.1",
    "express": "^4.16.4",
    "isomorphic-git": "^0.52.1",
